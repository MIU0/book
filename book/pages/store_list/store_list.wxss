page{
  background:#f9f9f9; 
}
.all_store{
  background: #fefefe;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.all_store_list{
width: 92%;
height: 232rpx;
display: flex;
  flex-direction: row;
  align-items: center;  
  border-bottom: 1px solid #eeeeee;
}
.store_list_left{
 width: 38%;
 height: 100%;
display: flex;
align-items: center;
margin-right: 10rpx;
}
.store_list_left image{
width: 100%;
 height: 68%;
}
.store_list_right{
 width: 60%;
 height: 100%;
}
.store_name{
  width: 100%;
  margin-top: 20rpx;
  color: #383838;
  font-size: 28rpx;
  overflow: hidden; 
   text-overflow: ellipsis; 
   display: -webkit-box;
   word-break: break-all; 
   -webkit-line-clamp:2;
   -webkit-box-orient: vertical; 
}
.store_bottom{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
}
.store_bottom_price{
  color: #ff484e;
}
.store_bottom_img{
  width: 60rpx;
  height: 60rpx;
}
.store_bottom_img image{
  width: 100%;
  height: 100%;
}
.no_more{
  margin-top: 10rpx;
  width: 100%;
text-align: center;
height: 60rpx;
line-height: 60rpx;
color: #c2c2c2;
font-size: 26rpx;
}